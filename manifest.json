{
  "manifest_version": 2,
  "name": "VK Audio To Text Translator",
  "version": "1.0.0",
  "browser_action": {
    "default_icon": "icon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://vk.com/im*"
      ],
      "js": ["jquery-3.5.1.min.js", "content.js"]
    }
  ],
  "background": {
    "scripts": ["background.js"]
  }
}